{
  "name": "universal-integration-framework",
  "version": "0.1.0",
  "description": "A TypeScript framework for building marketplace integrations with major developer platforms",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepare": "npm run build",
    "demo": "ts-node --project tsconfig.examples.json -r tsconfig-paths/register examples/demo.ts",
    "example:basic": "ts-node --project tsconfig.examples.json -r tsconfig-paths/register examples/basic-integration.ts",
    "example:github": "ts-node --project tsconfig.examples.json -r tsconfig-paths/register examples/github-integration.ts",
    "example:vercel": "ts-node --project tsconfig.examples.json -r tsconfig-paths/register examples/vercel-integration.ts",
    "example:custom": "ts-node --project tsconfig.examples.json -r tsconfig-paths/register examples/custom-integration.ts"
  },
  "keywords": [
    "integration",
    "oauth",
    "marketplace",
    "developer-tools",
    "typescript",
    "github",
    "vercel",
    "netlify",
    "webhooks"
  ],
  "author": "Universal Integration Framework",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/universal-integration-framework.git"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "crypto": "^1.0.1",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "@types/react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    }
  }
} 